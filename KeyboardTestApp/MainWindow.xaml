<Window x:Class="KeyboardTestApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="364" Width="525">
    <StackPanel>
        <Label x:Name="lbl1" Content="Label"/>
        <TextBox x:Name="click" Height="240" TextWrapping="Wrap" Text="TextBox" Margin="0,0,-0.333,0" TouchDown="TextBox_TouchDown" LostFocus="click_LostFocus" PreviewMouseUp="click_PreviewMouseUp" MouseWheel="click_MouseWheel"/>
        <Button Content="Button" Height="41" Margin="0,0,-0.333,0" Click="Button_Click" MouseLeftButtonDown="Button_MouseLeftButtonDown" MouseDown="Button_MouseDown" MouseMove="Button_MouseMove" MouseUp="Button_MouseUp" PreviewMouseDown="Button_PreviewMouseDown"/>
        <Button Content="Button" Height="25" Click="Button_Click_1"/>
    </StackPanel>
</Window>
